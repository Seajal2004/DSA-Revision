//Brute:
class Solution {
public:
    ListNode* reverseList(ListNode* head) {
        ListNode* temp = head;
        stack<int> st;
        while(temp != NULL){
            st.push(temp->val);
            temp = temp->next;
        }
        ListNode* temp1 = head;
        while(temp1 != NULL){
            temp1->val = st.top();
            st.pop();
            temp1 = temp1->next;
        }
        return head;
        
    }
};
