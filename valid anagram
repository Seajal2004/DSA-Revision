class Solution {
public:
    bool isAnagram(string s, string t) {
        vector<int> count(26,0);
        if(s.size() != t.size()) return false;
        for(char& ch : s){
            count[ch-'a']++;

        }
        for(char& ch : t){
            count[ch-'a']--;
        }
        for(int& num : count){
            if(num != 0){
                return false;
            }
        }
        return true;
        
    }
};
